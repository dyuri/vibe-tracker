{
  "name": "vibe-tracker-gemini",
  "version": "1.0.0",
  "description": "A comprehensive location tracker web application built with Go, PocketBase, and Leaflet.js. Track your journeys, manage sessions, and share your adventures with others.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "prepare": "husky",
    "analyze": "BUILD_ANALYZE=1 vite build",
    "size": "npm run build && du -sh dist/ && gzip -c dist/assets/*.js | wc -c | xargs -I {} echo 'Gzipped JS size: {} bytes'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyuri/vibe-tracker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/dyuri/vibe-tracker/issues"
  },
  "homepage": "https://github.com/dyuri/vibe-tracker#readme",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/leaflet": "^1.9.20",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@web/test-runner": "^0.20.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0",
    "workbox-build": "^7.3.0",
    "workbox-cli": "^7.3.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts": [
      "bash -c 'npx tsc --noEmit --skipLibCheck'"
    ],
    "*.{json,html,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "leaflet": "^1.9.4"
  }
}
